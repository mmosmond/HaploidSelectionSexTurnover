      RowBox[{"0", "<", "wAf"}], ",", 
      RowBox[{"0", "<", "waf"}], ",", 
      RowBox[{"0", "<", "\[Alpha]m", "<", "1"}], ",", 
      RowBox[{"0", "<", "\[Alpha]f", "<", "1"}]}], "}"}], "]"}]}], 
